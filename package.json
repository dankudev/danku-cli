{
	"name": "danku",
	"description": "DANKU CLI",
	"version": "0.0.1",
	"author": "Daniil Kubarenko",
	"bin": {
		"danku": "./bin/run.js"
	},
	"bugs": "https://github.com/dankudev/danku-cli/issues",
	"dependencies": {
		"@oclif/core": "^4.5.1",
		"@oclif/plugin-help": "^6.2.31",
		"@oclif/plugin-plugins": "^5.4.45",
		"arktype": "^2.1.20",
		"cloudflare": "^4.5.0",
		"jsonc-parser": "^3.3.1",
		"libsodium-wrappers": "^0.7.15",
		"octokit": "^3.2.2",
		"tldts": "^7.0.10",
		"yaml": "^2.8.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "^4.1.13",
		"@types/chai": "^5.2.2",
		"@types/libsodium-wrappers": "^0.7.14",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.16.5",
		"chai": "^5.2.1",
		"eslint": "^9.31.0",
		"eslint-config-oclif": "^6.0.86",
		"eslint-config-prettier": "^10.1.8",
		"mocha": "^11.7.1",
		"oclif": "^4.21.0",
		"prettier": "^3.6.2",
		"shx": "^0.3.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=22.14.0"
	},
	"files": [
		"./bin",
		"./dist",
		"./oclif.manifest.json",
		"templates"
	],
	"homepage": "https://danku.dev/svelte/cli",
	"keywords": [
		"danku",
		"framework"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"type": "module",
	"oclif": {
		"bin": "danku",
		"dirname": "danku",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-plugins"
		],
		"hooks": {
			"prerun": "./dist/hooks/prerun/configuration"
		}
	},
	"repository": "dankudev/danku-cli",
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint .",
		"format": "prettier --write .",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "pnpm run lint",
		"prepack": "oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"types": "dist/index.d.ts"
}
